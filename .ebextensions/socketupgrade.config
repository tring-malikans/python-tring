container_commands:
  aaacustomize:
    command: echo 'aaacustomize'
  bbbcustomize:
    command: twistd -ny application.tac
  ccccustomize:
    command: echo 'ccccustomize'
  enable_websockets:
    command: |
     sed -i '/\s*proxy_set_header\s*Connection/c \
              proxy_set_header Upgrade $http_upgrade;\
              proxy_set_header Connection "upgrade";\
        /tmp/deployment/config/#etc#nginx#conf.d#00_elastic_beanstalk_proxy.conf '
packages:
  yum:
      libffi-devel: []